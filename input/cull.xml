<program>
	<shader type="fragment" file="../glsl/coordinates.frag"/>
	<cull faces="front">
		<translate x="-2">
			<box size="2"/>
		</translate>
	</cull>
	<cull faces="back">
		<translate x="+2">
			<box size="2"/>
		</translate>
	</cull>
</program>
