<program>
	<shader file="../../glsl/coordinates.vert" />
	<shader file="../../glsl/coordinates.frag" />
	<depth mode="greater">
		<translate x="+3.0">
			<cube style="2D">
				<uniform type="mat4" name="MVPMatrix" />
			</cube>
		</translate>
		<translate x="-3.0">
			<cube style="2D">
				<uniform type="mat4" name="MVPMatrix" />
			</cube>
		</translate>
	</depth>
</program>
